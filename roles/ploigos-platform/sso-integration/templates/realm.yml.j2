apiVersion: keycloak.org/v1alpha1
kind: KeycloakRealm
metadata:
  name: openshift
  namespace: {{ ploigos_namespace }}
  labels:
    app: {{ rhsso_app_label }}
spec:
  realm:
    id: openshift
    realm: openshift
    enabled: True
    displayName: Ploigos OpenShift Realm
  instanceSelector:
    matchLabels:
      app: {{ rhsso_app_label }}
